<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0af"
    tools:context="in.ac.abes.trikumbh.trikumbh.TeamFragment">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:textColor="#ffffff" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/LinearLayout02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/activity_horizontal_margin"
                    android:layout_marginTop="@dimen/activity_horizontal_margin" >

                    <ImageView
                        android:id="@+id/ImageView05"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_marginLeft="@dimen/activity_vertical_margin"
                        android:layout_weight="0.26"
                        android:scaleType="fitXY"
                        android:src="@drawable/team1" />

                    <ImageView
                        android:id="@+id/ImageView04"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_marginLeft="@dimen/activity_vertical_margin"
                        android:layout_marginRight="@dimen/activity_vertical_margin"
                        android:scaleType="fitXY"
                        android:src="@drawable/team2" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/activity_horizontal_margin"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/TextView04"
                        android:layout_width="110dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@+id/linearLayout1"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_weight="0.05"
                        android:ems="10"
                        android:gravity="center_horizontal"
                        android:text="CHIRAG GUPTA \n EVENT HEAD"
                        android:textColor="#ffffff"
                        android:textStyle="italic" />

                    <TextView
                        android:id="@+id/TextView05"
                        android:layout_width="115dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_weight="0.05"
                        android:ems="10"
                        android:gravity="center_horizontal"
                        android:text="ROHAN GUPTA \n EVENT HEAD"
                        android:textColor="#ffffff"
                        android:textStyle="italic" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/activity_horizontal_margin"
                    android:layout_marginRight="@dimen/activity_vertical_margin" >

                    <ImageView
                        android:id="@+id/ImageView07"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_margin="@dimen/activity_vertical_margin"
                        android:layout_weight="0.60"
                        android:scaleType="fitXY"
                        android:src="@drawable/mcas" />

                    <ImageView
                        android:id="@+id/ImageView01"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_margin="@dimen/activity_vertical_margin"
                        android:layout_weight="0.60"
                        android:scaleType="fitXY"
                        android:src="@drawable/team3" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center_horizontal"
                        android:text="SHIVANSHU\nCOORDINATOR"
                        android:textColor="#ffffff"
                        android:textStyle="italic" />

                    <TextView
                        android:id="@+id/TextView9504"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@+id/linearLayout1"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center_horizontal"
                        android:text="SANYUKTA SHARMA\n EVENT HEAD"
                        android:textColor="#ffffff"
                        android:textStyle="italic" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/activity_horizontal_margin"
                android:layout_marginTop="@dimen/activity_horizontal_margin"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/activity_horizontal_margin"
                    android:layout_marginRight="@dimen/activity_vertical_margin" >

                    <ImageView
                        android:id="@+id/ImageView707"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_margin="@dimen/activity_vertical_margin"
                        android:layout_weight="0.60"
                        android:scaleType="fitXY"
                        android:src="@drawable/chona" />
                    <ImageView
                        android:id="@+id/ImageView807"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_margin="@dimen/activity_vertical_margin"
                        android:layout_weight="0.60"
                        android:scaleType="fitXY"
                        android:src="@drawable/team7" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/activity_horizontal_margin"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/TextView904"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@+id/linearLayout1"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center_horizontal"
                        android:text="SIDDHARTH CHONA \nCOORDINATOR"
                        android:textColor="#ffffff"
                        android:textStyle="italic" />
                    <TextView
                        android:id="@+id/TextView704"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@+id/linearLayout1"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center_horizontal"
                        android:text="ABHISHEK \nCOORDINATOR"
                        android:textColor="#ffffff"
                        android:textStyle="italic" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/activity_horizontal_margin"
                android:layout_marginTop="@dimen/activity_horizontal_margin" >
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout01"
            android:layout_width="192dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_weight="1"
            android:orientation="vertical" >
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_vertical_margin" >

            <ImageView
                android:id="@+id/ImageView06"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_weight="0.60"
                android:scaleType="fitXY"
                android:src="@drawable/ritik" />

            <ImageView
                android:id="@+id/ImageView087"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_margin="@dimen/activity_vertical_margin"
                android:layout_weight="0.60"
                android:scaleType="fitXY"
                android:src="@drawable/team4" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_horizontal_margin"
            android:gravity="center_horizontal"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/TextView505"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:text="RITIK GUPTA\nCOORDINATOR"
                android:textColor="#ffffff"
                android:textStyle="italic" />

            <TextView
                android:id="@+id/TextView204"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/linearLayout1"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="15dp"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_horizontal"
                android:text="RAHUL\nWEB TEAM"
                android:textColor="#ffffff"
                android:textStyle="italic" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/activity_horizontal_margin"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/activity_horizontal_margin"
                android:layout_marginRight="@dimen/activity_vertical_margin" >

                <ImageView
                    android:id="@+id/ImageView026"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginTop="@dimen/activity_vertical_margin"
                    android:layout_weight="0.60"
                    android:scaleType="fitXY"
                    android:src="@drawable/shu" />

                <ImageView
                    android:id="@+id/ImageView016"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_marginTop="@dimen/activity_vertical_margin"
                    android:layout_weight="0.60"
                    android:scaleType="fitXY"
                    android:src="@drawable/team6" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/activity_horizontal_margin"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/TextView052"
                    android:layout_width="115dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.05"
                    android:ems="10"
                    android:gravity="center_horizontal"
                    android:text="SHUDHANSHU \n CS SECOND YEAR"
                    android:textColor="#ffffff"
                    android:textStyle="italic" />

                <TextView
                    android:id="@+id/TextView051"
                    android:layout_width="115dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_weight="0.05"
                    android:ems="10"
                    android:gravity="center_horizontal"
                    android:text="VAIBHAV SAHU\nWEB TEAM"
                    android:textColor="#ffffff"
                    android:textStyle="italic" />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</ScrollView>


